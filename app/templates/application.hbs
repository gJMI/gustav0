<svg id="SvgjsSvg1006" width="92" height="92" xmlns="http://www.w3.org/2000/svg" version="1.1" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:svgjs="http://svgjs.com/svgjs"><defs id="SvgjsDefs1007"><filter id="SvgjsFilter1010"><feOffset id="SvgjsFeOffset1011" dx="5" dy="6" result="SvgjsFeOffset1011Out" in="SourceAlpha"></feOffset><feGaussianBlur id="SvgjsFeGaussianBlur1012" stdDeviation="5 5" result="SvgjsFeGaussianBlur1012Out" in="SvgjsFeOffset1011Out"></feGaussianBlur><feBlend id="SvgjsFeBlend1013" in="SourceGraphic" in2="SvgjsFeGaussianBlur1012Out" mode="normal" result="SvgjsFeBlend1013Out"></feBlend></filter></defs><text id="SvgjsText1008" font-family="Indie Flower" x="42" y="-1.796875" font-size="50" text-anchor="middle" fill="#222223" filter="url(#SvgjsFilter1010)"><tspan id="SvgjsTspan1009" dy="65" x="42">JKL</tspan></text><rect id="SvgjsRect1014" width="15" height="15" x="92" y="92" fill="#ff0033"></rect></svg>

  


<div class="w3-main">

  <span class="w3-opennav w3-xlarge" onclick="w3_open()"><i class="fa fa-bars"></i></span>
  <div class="w3-row">
    <div class="w3-col s10 w3-left">
      <h3><div class="w3-container w3-animate-left w3-pale-blue">
      {{t "user.welcome" firstname=profile.firstname lastname=profile.lastname salutation=profile.salutation}}
      </div></h3>
    </div>
    <div class="w3-col s2 w3-center">
      <div class="w3-card-4" style="width:50%">
      <div id="userimage"></div>  
       {{#if userimage}}
        <img src="data:image/gif;base64,{{userimage.image}}" class="w3-border" alt="{{t 'user.profilePhoto'}}">
      {{else}}
      {{--debugger}}
     <script>
        var draw = SVG('userimage').size(92, 92);
        var text = draw.text('{{profile.monogram}}').move(42, 5);
        text.font({
          family: 'Indie Flower',
          size: 50,
          anchor: 'middle',
          fill: '#222223'
        });
        text.filter(function(add) {
         var blur = add.offset(5, 6).in(add.sourceAlpha).gaussianBlur(5);
         add.blend(add.source, blur);
        });
        draw.rect(15, 15).animate().fill('#f03').move(92, 92);
      </script>
      {{/if}}
      </div>
    </div>
  </div>
  {{#link-to "overview" current-when="overview"}} {{/link-to}} {{outlet}}
</div>
<div class="w3-container w3-pale-blue">
  <p>Developed in 2016</p>
</div>